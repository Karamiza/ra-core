        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-config-react-app": "^4.0.1",
        "eslint-plugin-cypress": "^2.2.1",
        "eslint-plugin-flowtype": "^3.9.1",
